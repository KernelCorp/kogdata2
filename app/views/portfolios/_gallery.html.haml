.gallery
  - if owner.image_portfolios.blank?
    .portfolio_empty
      = t(:portfolio_is_empty)
  -else
    %ul{:class=>'elastislide-list', :id=>'carousel'}
      -owner.image_portfolios.each do |image|
        %li
          %a{href: '#', class: 'portfolio_image_link', 'data-preview' => image.src.url(:original)}
            = image_tag image.src.url(:small), alt: 'image'
